{
    "name": "kaishiyoku/nfze",
    "description": "Source code of the website nfze.de",
    "keywords": ["website", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.4.30",
        "laravel/tinker": "1.0.2",
        "commerceguys/enum": "1.0",
        "laravelcollective/html": "5.4.8",
        "austinb/gameq": "3.0.11",
        "kaishiyoku/laravel-menu": "1.3.6"
    },
    "require-dev": {
        "fzaninotto/faker": "1.6.0",
        "mockery/mockery": "0.9.9",
        "phpunit/phpunit": "6.2.3",
        "barryvdh/laravel-ide-helper": "2.4.0",
        "doctrine/dbal": "2.5.12"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:models --dir=\"app/Models\" --write",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
