{
  "name": "kaishiyoku/nfze",
  "description": "Source code of the website nfze.de",
  "keywords": [
    "website",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.0.0",
    "laravel/framework": "5.5.25",
    "laravel/tinker": "1.0.2",
    "commerceguys/enum": "1.0",
    "laravelcollective/html": "5.5.1",
    "austinb/gameq": "3.0.12",
    "kaishiyoku/laravel-menu": "1.4.1"
  },
  "require-dev": {
    "fzaninotto/faker": "1.7.1",
    "mockery/mockery": "1.0",
    "phpunit/phpunit": "6.5.5",
    "barryvdh/laravel-ide-helper": "2.4.1",
    "doctrine/dbal": "2.5.12",
    "filp/whoops": "2.1.14"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta",
      "php artisan ide-helper:models --dir=\"app/Models\" --write"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
